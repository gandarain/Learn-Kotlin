<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="@color/palletNormalBorder"/>
            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke
                android:width="2dp"
                android:color="@color/palletNormalBorder"/>
            <solid android:color="@color/palletBackground"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
</layer-list>